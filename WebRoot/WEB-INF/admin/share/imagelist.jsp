<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="${pageContext.request.contextPath}/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="${pageContext.request.contextPath}/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><form action="./save-image-list.jspx" method="post" enctype="multipart/form-data" class="form form-horizontal" id="form-submit">		<input type="hidden" name="shareid" value="${shareid}"  />		<div class="row cl">			<label class="form-label col-2"><span class="c-red">*</span>图片：</label>			<div class="formControls col-10">				<input type="file" class="input-text" required="required"  placeholder="" name="file">			</div>		</div>		<div class="row cl">			<div class="col-10 col-offset-2">				<button class="btn btn-primary radius" type="submit"><i class="Hui-iconfont">&#xe632;</i> 保存</button>			</div>		</div>				<div style="height: 100px;"></div></form><script type="text/javascript">$(function(){	$("#form-submit").Validform({		tiptype:3,		callback:function(form){			form[0].submit();		}	});});</script><div style="clear: both;"></div><div style="float: left;width:100%;margin: 10px;"><c:forEach items="${list}" var="item">	<div style="margin: 5px;float: left;border: 2px solid #eee;">		<img alt="" src="${pageContext.request.contextPath}/image.jspx?i=${item.imageurl}" width="200" height="200"/>		<a href="#" onclick="del('${item.imageid}','${item.shareid}')">删除</a>	</div></c:forEach>	     </div><script type="text/javascript">function del(imageid,shareid){	layer.confirm('确认要删除吗？',function(index){	 	window.location.href="./delete-image.jspx?imageid="+imageid+"&shareid="+shareid;	});}</script></body></html>