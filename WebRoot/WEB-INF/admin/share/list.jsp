<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib prefix="sp" uri="http://mos.xinfan.com/"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><!DOCTYPE html><html lang="en"><head>      <jsp:include page="../head.jsp"></jsp:include><script type="text/javascript" src="${pageContext.request.contextPath}/jslib/uiadmin/lib/My97DatePicker/WdatePicker.js"></script><script type="text/javascript" src="${pageContext.request.contextPath}/jslib/uiadmin/lib/datatables/1.10.0/jquery.dataTables.min.js"></script></head><body><nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span>分享管理<span class="c-gray en">&gt;</span>分享列表 <a class="btn btn-success radius r mr-20" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a></nav><div class="pd-20">		<div class="text-c"> 	</div>	<div class="cl pd-5 bg-1 bk-gray mt-20"> 		<span class="l"><a class="btn btn-primary radius" onclick="add()" href="javascript:;">		<i class="Hui-iconfont">&#xe600;</i> 添加分享</a></span> </div>	<div class="mt-20">	<table class="table table-border table-bordered table-hover table-bg table-sort">		<thead>			<tr class="text-c">                 <th>影片ID</th>                 <th>标题</th>                 <th>创建时间</th>                 <th>在线状态</th>                 <th>操作</th>             </tr>		</thead>		<tbody>			<c:forEach items="${list}" var="item">	           <tr> 	           		<td>${item.shareid}</td> 	           		<td>${item.title }</td> 	           		<td><sp:dateformat time="${item.createdate}"/></td> 	           		<td>	           			<sp:dict_label_tag type="share_online" value="${item.online}"></sp:dict_label_tag>	           		</td> 	           		<td>	           			<a href="">查看</a>	           			<a href="">下线</a>	           			<a href="">上线</a>	           			<a href="">删除</a>	           		</td> 	           </tr> 				</c:forEach>	           		               			</tbody>	</table>	</div></div><script type="text/javascript">	function reload(){	 datatable.fnClearTable(0);       datatable.fnDraw(); //重新加载数据 }function getParamter(data){	var name = $("#name").val();	var word = $("#word").val();	data['name']=name;	data['word']=word;}function add(){	var index = layer.open({		type: 2,		title: '添加片源',		content: '${pageContext.request.contextPath}/admin/keymovie/add.jspx'	});	layer.full(index);}function setkeyword(id){	layer_show('修改分类','${pageContext.request.contextPath}/admin/keymovie/update.jspx?movieId='+id,600,370);	}/*删除*/function movie_delete(obj,id){	layer.confirm('确认要删除吗？',function(index){	  var _data = {"movieId": id};      $.ajax({type:"POST",             url:"delete.jspx?t="+new Date().getTime(),             data:_data,             dataType:"json",             success:function(data){            	if(data.result ==0){            		layer.msg('已删除!',{icon:1,time:1000});            		reload();            	}            	else{            		layer.msg(data.message,{icon:1,time:1000});            	}             }		});	});}</script></body></html>